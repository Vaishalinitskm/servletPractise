<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet> 
   <servlet-name>GenServ</servlet-name>
   <servlet-class>com.practise.DemoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>GenServ</servlet-name>
   <url-pattern>/genServTest</url-pattern>
  </servlet-mapping>
  
  <servlet> 
   <servlet-name>HttpServ</servlet-name>
   <servlet-class>com.practise.DemoHttp</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>HttpServ</servlet-name>
   <url-pattern>/httpServTest</url-pattern>
  </servlet-mapping>
  
  <servlet> 
   <servlet-name>HttpServ2</servlet-name>
   <servlet-class>com.practise.DemoHttp2</servlet-class>
  </servlet>
  <servlet-mapping>
   <servlet-name>HttpServ2</servlet-name>
   <url-pattern>/httpServTest2</url-pattern>
  </servlet-mapping>
</web-app>